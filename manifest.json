{
    "name": "__MSG_extName__",
    "version": "0.2.0",
    "manifest_version": 2,
    "description": "__MSG_extDesc__",
    "homepage_url": "https://github.com/sameerdash2/javascript-blocker",
    "default_locale": "en",
    "permissions": [
        "activeTab",
        "contentSettings",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+J"
            }
        }
    }
}
